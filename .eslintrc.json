{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "plugins": ["import"],
    "extends": ["plugin:prettier/recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "warn",
        "react/jsx-key": "warn",
        "no-console": "warn",
        "no-eval": "warn",
        "import/first": "warn",
        "no-unused-vars": "warn",
        "no-async-promise-executor": "warn"
    }
}
